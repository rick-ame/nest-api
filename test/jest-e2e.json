{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/../src/$1"
  },
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "reporters": [
    "default",
    [
      "jest-junit",
      {
        "outputName": "junit-e2e.xml"
      }
    ]
  ]
}
